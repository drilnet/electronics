// ---
// | Очистить input (только один input).
// ---
function ClearInput(idElement)
{
	document.getElementById(idElement).value = "";
}

// ---
// | Очистить input (несколько input'ов).
// ---
function ClearInputArray(idElementArray)
{
	var index;

	for(index = 0; index < idElementArray.length; ++index)
		{
		document.getElementById(idElementArray[index]).value = "";
		}
}

// ---
// | Онлайн калькулятор CALCDIVR2_Z1
// | Считаем!
// ---
function CALCDIVR2_Z1()
{
	var Uin = Number(document.getElementById("z1_input_1").value);
		var R1 = Number(document.getElementById("z1_input_2").value);
			var R2 = Number(document.getElementById("z1_input_3").value);

	var Option1 = document.getElementById("z1_option_1").value;
	var Option2 = document.getElementById("z1_option_2").value;
	var Option3 = document.getElementById("z1_option_3").value;

	if (Option1 == 'мВ')
		{
		Uin = Uin / 1000; // мВ в В.
		}

	if (Option1 == 'мкВ')
		{
		Uin = Uin / 1000 / 1000; // мкВ в В.
		}

	if (Option2 == 'кОм')
		{
		R1 = R1 * 1000; // кОм в Ом.
		}

	if (Option2 == 'мОм')
		{
		R1 = R1 * 1000 * 1000; // мОм в Ом.
		}

	if (Option3 == 'кОм')
		{
		R2 = R2 * 1000; // кОм в Ом.
		}

	if (Option3 == 'мОм')
		{
		R2 = R2 * 1000 * 1000; // мОм в Ом.
		}

	// Вычисления.

	var Uout1 = R1 / (R1 + R2) * Uin;
	var Uout2 = R2 / (R1 + R2) * Uin;

	var P1 = Uout1 * Uout1 / R1;
	var P2 = Uout2 * Uout2 / R2;

	var I = Uin / (R1 + R2);

	// Результат на HTML-страницу.

	document.getElementById('z1_result_Uout1').innerHTML =
		'<span class="calcdivcolorresult">'
		+ Uout1 * 1000 * 1000 + '&nbsp;<b>мкВ</b>,&nbsp;'
			+ Uout1 * 1000 + '&nbsp;<b>мВ</b>,&nbsp;'
				+ Uout1 + '&nbsp;<b>В</b>.'
					+ '</span>';

	document.getElementById('z1_result_Uout2').innerHTML =
		'<span class="calcdivcolorresult">'
		+ Uout2 * 1000 * 1000 + '&nbsp;<b>мкВ</b>,&nbsp;'
			+ Uout2 * 1000 + '&nbsp;<b>мВ</b>,&nbsp;'
				+ Uout2 + '&nbsp;<b>В</b>.'
					+ '</span>';

	document.getElementById('z1_result_P1').innerHTML =
		'<span class="calcdivcolorresult">'
		+ P1 * 1000 + '&nbsp;<b>мВт</b>,&nbsp;'
			+ P1 + '&nbsp;<b>Вт</b>.'
				 + '</span>';

	document.getElementById('z1_result_P2').innerHTML =
	'<span class="calcdivcolorresult">'
		+ P2 * 1000 + '&nbsp;<b>мВт</b>,&nbsp;'
			+ P2 + '&nbsp;<b>Вт</b>.'
				 + '</span>';

	document.getElementById('z1_result_I').innerHTML =
	'<span class="calcdivcolorresult">'
		+ I * 1000 * 1000 + '&nbsp;<b>мкА</b>,&nbsp;'
			+ I * 1000 + '&nbsp;<b>мА</b>,&nbsp;'
				+ I + '&nbsp;<b>А</b>.'
					+ '</span>';
}

